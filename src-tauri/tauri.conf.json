{
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "devPath": "http://localhost:3000",
    "distDir": "../out"
  },
  "package": {
    "productName": "notor",
    "version": "0.1.1"
  },
  "tauri": {
    "systemTray": {
      "iconPath": "icons/icon.png",
      "iconAsTemplate": true
    },
    "allowlist": {
      "all": false,
      "window": {
        "startDragging": false,
        "setFullscreen": true
      },
      "shell": {
        "all": false,
        "open": true
      },
      "notification": {
        "all": true
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "resources/icons/32x32.png",
        "resources/icons/128x128.png",
        "resources/icons/128x128@2x.png",
        "resources/icons/icon.icns",
        "resources/icons/icon.ico"
      ],
      "identifier": "com.notor.app",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [
        "resources/icons/32x32.png",
        "resources/*"
      ],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null,
      "dangerousRemoteDomainIpcAccess": [
        {
          "domain": "tauri.localhost",
          "enableTauriAPI": true,
          "windows": [
            "main",
            "auth",
            "alert"
          ]
        },
        {
          "domain": "notor.vercel.app",
          "enableTauriAPI": true,
          "windows": [
            "main",
            "auth",
            "alert"
          ]
        }
      ]
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "resizable": false,
        "title": "events",
        "width": 300,
        "decorations": false
      }
    ]
  }
}